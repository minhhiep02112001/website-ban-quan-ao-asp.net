    @RenderSection("cssHeader", required: false)
    <!-- Main CSS-->
    <link href="~/Public/admin/css/theme.css" rel="stylesheet" media="all">
    <style>
        .position-absolute {
            position: absolute;
        }
    </style>
</head>

<body class="animsition">
    <div class="page-wrapper">
        @RenderPage("_SideBarMobile.cshtml")
        @RenderPage("_SideBar.cshtml")
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            @RenderPage("_Header.cshtml")
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <div class="main-content">
                @RenderBody()
            </div>
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>

    </div>

    <script>
        function onchange_ten(val) {
            var str = val.value;
            document.getElementById("SLUG").value = removeVietnameseTones(str);
        }
    </script>
    <!-- Jquery JS-->
    @* <script src="~/Public/admin/vendor/jquery-3.2.1.min.js"></script>*@
    <!-- Bootstrap JS-->
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Public/admin/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="~/Public/admin/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- ~/Public/admin/vendor JS       -->
    <script src="~/Public/admin/vendor/slick/slick.min.js"></script>
    <script src="~/Public/admin/vendor/wow/wow.min.js"></script>
